proc %foo (i32$ %a, i32$ %b) -> (i32$ %s, i32$ %c) {
entry:
    %a0 = prb i32$ %a
    %b0 = prb i32$ %b
    %0 = xor i32 %a0, %b0
    %1 = and i32 %a0, %b0
    %dt = const time 0s 1d
    drv i32$ %s, %0, %dt
    drv i32$ %c, %1, %dt
    wait %entry, %a, %b
}

